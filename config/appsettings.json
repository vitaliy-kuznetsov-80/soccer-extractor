{
  // Общие настройки
  general: {
    page_load_timeout: 10, // Таймаут на загрузку страницы, сек
    element_load_timeout: 5, // Таймаут на загрузку элемента, сек

    // Политика повторения для загрузки страницы
    retry_count: 3, // Количество повторений
    retry_period: 20, // Период между повторениями, сек
  },

  // Дебаг режим
  debug: {
    log_in_console: false, // true - отображать логи в консоли, false - отображать в файле
    lines_limit: 0, // Количество линий игр для считывания, 0 - не ограничивать
    only_line_id: '', // Идентификатор линии для считывания, '' - не ограничивать
    only_game_id: '', // Идентификатор игры для считывания, '' - не ограничивать
  },

  // Планировщик
  scheduler: {
    // Смещение дня вперед для формирования даты файла Excel, часы
    // Например: 3 - начало следующего дня в 21:00
    day_offset: 3,
    // Времена старта считывания
    europe: '06:00', // Еврома
    america: '12:30', // Америка
    asia: '22:30', // Азия
    results: '13:00', // Результаты
  },
}